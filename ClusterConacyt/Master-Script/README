for num in {325..348}; do perl cleanConsensus_v4.pl ensamblajes/${num}_S${count}.fa variantes/${num}_S${count}.tsv ensamblajes/${num}_S${count}-clean.fa reference-covid19.fasta; count=$((count+1)); done

